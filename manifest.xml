<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xsi:type="MailApp">
  <Id>aec2521c-0032-4169-a208-dba23c573030</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>AIDU</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="AIDU"/>
  <Description DefaultValue="Turn your inbox into a sales weapon with AIDU"/>
  <IconUrl DefaultValue="https://aidu.ai/outlook-add-in/assets/icon-64.png"/>
  <HighResolutionIconUrl DefaultValue="https://aidu.ai/outlook-add-in/assets/icon-128.png"/>
  <SupportUrl DefaultValue="https://aidu.ai/contact"/>
  <AppDomains>
    <AppDomain>https://aidu.ai</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Mailbox"/>
  </Hosts>
  <Requirements>
    <Sets>
      <Set Name="MailBox" MinVersion="1.1"/>
    </Sets>
  </Requirements>
  <FormSettings>
    <Form xsi:type="ItemRead">
      <DesktopSettings>
        <SourceLocation DefaultValue="https://aidu.ai/outlook-add-in/taskpane.html"/>
        <RequestedHeight>250</RequestedHeight>
      </DesktopSettings>
    </Form>
  </FormSettings>
  <Permissions>ReadWriteItem</Permissions>
  <Rule xsi:type="RuleCollection" Mode="Or">
    <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read"/>
    <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Read"/>
  </Rule>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides" xsi:type="VersionOverridesV1_0">
    <VersionOverrides xmlns="http://schemas.microsoft.com/office/mailappversionoverrides/1.1" xsi:type="VersionOverridesV1_1">
      <Hosts>
        <Host xsi:type="MailHost">
          <DesktopFormFactor>
            <SupportsSharedFolders>true</SupportsSharedFolders>
            <ExtensionPoint xsi:type="MessageReadCommandSurface">
              <OfficeTab id="TabDefault">
                <Group id="CommandsGroup">
                  <Label resid="CommandsGroup.Label"/>
                  <Control xsi:type="Button" id="TaskpaneButton">
                    <Label resid="TaskpaneButton.Label"/>
                    <Supertip>
                      <Title resid="TaskpaneButton.Label"/>
                      <Description resid="TaskpaneButton.Tooltip"/>
                    </Supertip>
                    <Icon>
                      <bt:Image size="16" resid="Icon.16x16"/>
                      <bt:Image size="32" resid="Icon.32x32"/>
                      <bt:Image size="80" resid="Icon.80x80"/>
                    </Icon>
                    <Action xsi:type="ShowTaskpane">
                      <SourceLocation resid="Taskpane.Url"/>
                      <SupportsPinning>true</SupportsPinning>
                    </Action>
                  </Control>
                </Group>
              </OfficeTab>
            </ExtensionPoint>
          </DesktopFormFactor>
        </Host>
      </Hosts>
      <Resources>
        <bt:Images>
          <bt:Image id="Icon.16x16" DefaultValue="https://aidu.ai/outlook-add-in/assets/icon-16.png"/>
          <bt:Image id="Icon.32x32" DefaultValue="https://aidu.ai/outlook-add-in/assets/icon-32.png"/>
          <bt:Image id="Icon.80x80" DefaultValue="https://aidu.ai/outlook-add-in/assets/icon-80.png"/>
        </bt:Images>
        <bt:Urls>
          <bt:Url id="Commands.Url" DefaultValue="https://aidu.ai/outlook-add-in/commands.html"/>
          <bt:Url id="Taskpane.Url" DefaultValue="https://aidu.ai/outlook-add-in/taskpane.html"/>
        </bt:Urls>
        <bt:ShortStrings>
          <bt:String id="GetStarted.Title" DefaultValue="Welcome to AIDU"/>
          <bt:String id="CommandsGroup.Label" DefaultValue="AIDU Tools"/>
          <bt:String id="TaskpaneButton.Label" DefaultValue="Open AIDU"/>
        </bt:ShortStrings>
        <bt:LongStrings>
          <bt:String id="GetStarted.Description" DefaultValue="Turn your inbox into a sales weapon with AIDU"/>
          <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Open AIDU to enhance your email with AI"/>
        </bt:LongStrings>
      </Resources>
      <WebApplicationInfo>
        <Id>33b17c75-d913-41da-8560-cc3014322e50</Id>
        <Resource>api://aidu.ai/outlook-add-in/33b17c75-d913-41da-8560-cc3014322e50</Resource>
        <Scopes>
          <Scope>openid</Scope>
          <Scope>User.Read</Scope>
          <Scope>profile</Scope>
          <Scope>Mail.ReadWrite</Scope>
        </Scopes>
      </WebApplicationInfo>
    </VersionOverrides>
  </VersionOverrides>
</OfficeApp>